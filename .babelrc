{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@store": "./src/store",
          "@ui-components": "./src/ui-components",
          "@contexts": "./src/contexts",
          "@hooks": "./src/hooks",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}
